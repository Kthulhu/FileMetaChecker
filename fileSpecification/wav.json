{
	"standard": {
		"label": "Waveform Audio File Format",
		"id": "wav",
		"type": "audio",
		"extension": [
			".wav",
			".wave"
		]
	},
	"header": [
		{
			"id": "chunkId",
			"label": "File bloc id",
			"type": "ascii",
			"endian": "big",
			"values": "RIFF"
		},
		{
			"id": "chunkSize",
			"label": "File size",
			"type": "uint32",
			"endian": "little"
		},
		{
			"id": "riffType",
			"label": "Riff type id",
			"type": "ascii",
			"endian": "big",
			"values": "WAVE"
		},
		{
			"id": "specificWaveChunks",
			"label": "Specific WAVE chunks",
			"type": "ascii",					// @todo: change to "empty" type
			"ordered": false,
			"group": [
				{
					"id": "fmtSubChunks",
					"label": "Format sub-chunk",
					"type": "ascii",
					"values": "fmt ",
					"endian": "big",
					"optional": false,
					"groupSize": "fmtSize + 4",
					"group": [
						{
							"id": "fmtSize",
							"label": "Sub-chunk size",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "compressionCode",
							"label": "Compression code",
							"type": "uint16",
							"endian": "little",
							"map": [
								{ "0": "unknown" },
								{ "1": "PCM/uncompressed" },
								{ "2": "Microsoft ADPCM" },
								{ "6": "ITU G.711 a-law" },
								{ "7": "ITU G.711 Âµ-law" },
								{ "17": "IMA ADPCM" },
								{ "20": "ITU G.723 ADPCM" },
								{ "49": "GSM 6.10" },
								{ "64": "ITU G.721 ADPCM" },
								{ "80": "MPEG" },
								{ "65536": "Experimental" }
							]
						},
						{
							"id": "numChannels",
							"label": "Number of channels",
							"type": "uint16",
							"endian": "little"
						},
						{
							"id": "sampleRate",
							"label": "Sample rate",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "byteRate",
							"label": "Bytes per second",
							"type": "uint32",
							"endian": "little"
							// "expr": "sampleRate * blockAlign"
						},
						{
							"id": "blockAlign",
							"label": "Byte per block",
							"type": "uint16",
							"endian": "little"
							// "expr": "bitsPerSample / 8 * numChannels"
						},
						{
							"id": "bitsPerSample",
							"label": "Bits per sample",
							"type": "uint16",
							"endian": "little",
							"required": "compressionCode == 1"
						}
					]
				},
				{
					"id": "factSubChunks",
					"label": "Fact sub-chunk",
					"type": "ascii",
					"values": "fact",
					"endian": "big",
					"groupSize": "factSize + 4",
					"optional": true,
					"group": [
						{
							"id": "factSize",
							"label": "SubChunk size",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "dataSamples",
							"label": "Data samples number",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "formatDependantData",
							"label": "Format dependant data",
							"type": "raw",
							"count": "factSize - 4"
						}
					]
				},
				{
					"id": "dataSubChunks",
					"label": "Data sub-chunk",
					"type": "ascii",
					"values": "data",
					"endian": "big",
					"groupSize": "dataSize + 4",
					"optional": false,
					"group": [
						{
							"id": "dataSize",
							"label": "Sub-chunk size",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "sampleData",
							"label": "Sample data",
							"type": "raw",
							"count": "dataSize"
						}
					]
				},
				{
					"id": "wavlSubChunks",
					"label": "WAVE list sub-chunk",
					"type": "ascii",
					"values": "wavl",
					"groupSize": "wavlSize + 4",
					"optional": true,
					"group": [
						{
							"id": "wavlSize",
							"label": "WAVE list bloc size",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "sampleData",
							"label": "Sample data",
							"type": "raw",
							"count": "wavlSize"
						}
					]
				},
				{
					"id": "slntSubChunks",
					"label": "Silent sub-chunk",
					"type": "ascii",
					"values": "slnt",
					"groupSize": "slntSize + 4",
					"optional": true,
					"group": [
						{
							"id": "slntSize",
							"label": "Silent bloc size",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "silentSamples",
							"label": "Silent samples number",
							"type": "uint32",
							"endian": "little"
						}
					]
				},
				{
					"id": "cueSubChunks",
					"label": "Cue sub-chunk",
					"type": "ascii",
					"values": "cue ",
					"groupSize": "cueSize + 4",
					"optional": true,
					"group": [
						{
							"id": "cueSize",
							"label": "Cue bloc size",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "cuePoints",
							"label": "Cue points number",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "cuePointsList",
							"label": "Cue points list",
							"type": "raw",
							"count": "cueSize - 4",
							"displayType": "ascii"
						}
					]
				},
				{
					"id": "plstSubChunks",
					"label": "Playlist sub-chunk",
					"type": "ascii",
					"values": "plst",
					"groupSize": "plstSize + 4",
					"optional": true,
					"group": [
						{
							"id": "plstSize",
							"label": "Playlist bloc size",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "plstSegments",
							"label": "Playlist segments number",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "plstSegmentsList",
							"label": "Playlist segments list",
							"type": "raw",
							"count": "plstSize - 4"
						}
					]
				},
