					"values": "data",
					"endian": "big",
					"groupSize": "dataSize + 4 + dataSize%2",
					"optional": false,
					"group": [
						{
							"id": "dataSize",
							"label": "Sub-chunk size",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "sampleData",
							"label": "Sample data",
							"type": "raw",
							"size": "dataSize"
						}
					]
				},
				{
					"id": "wavlSubChunks",
