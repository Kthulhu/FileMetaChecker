{
	"description": {
		"label": "Extern Waveform Audio File Format",
		"id": "extwav",
		"type": "audio",
		"extension": [
			".wav",
			".wave"
		]
	},
	"content": [
		{
			"id": "specificWaveChunks",
			"label": "Specific WAVE chunks",
			"ordered": false,
			"group": [
				{
					"id": "junkSubChunks",
					"label": "JUNK sub-chunk",
					"type": "ascii",
					"values": "junk",
					"endian": "big",
					"optional": true,
					"groupSize": "junkSize + 4 + junkSize%2",
					"group": [
						{
							"id": "junkSize",
							"label": "Sub-chunk size",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "dummyBytes",
							"label": "Dummy bytes",
							"type": "raw",
							"size": "junkSize"
						}
					]
				},
				{
					"id": "fmtSubChunks",
					"label": "Format sub-chunk",
					"type": "ascii",
					"values": "fmt ",
					"endian": "big",
					"optional": false,
					"groupSize": "fmtSize + 4 + fmtSize%2",
					"group": [
						{
							"id": "fmtSize",
							"label": "Sub-chunk size",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "formatType",
							"label": "Format type (or Compression code)",
							"type": "uint16",
							"endian": "little",
							"map": [
								{ "0": "unknown" },
								{ "1": "PCM/uncompressed" },
								{ "2": "Microsoft ADPCM" },
								{ "3": "IEEE float" },
								{ "6": "8-bit ITU G.711 A-law" },
								{ "7": "8-bit ITU G.711 Âµ-law" },
								{ "17": "IMA ADPCM" },
								{ "20": "ITU G.723 ADPCM" },
								{ "49": "GSM 6.10" },
								{ "64": "ITU G.721 ADPCM" },
								{ "80": "MPEG" },
								{ "65536": "Experimental" }
							]
						},
						{
							"id": "channelCount",
							"label": "Number of channels",
							"type": "uint16",
							"endian": "little"
						},
						{
							"id": "sampleRate",
							"label": "Sample rate",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "byteRate",
							"label": "Bytes per second",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "blockAlignment",
							"label": "Byte per block",
							"type": "uint16",
							"endian": "little"
						},
						{
							"id": "bitsPerSample",
							"label": "Bits per sample",
							"type": "uint16",
							"endian": "little"
						},
						{
							"id": "cbSize",
							"label": "Extra size",
							"type": "uint16",
							"requirement": "formatType is not 1",
							"endian": "little"
						},
						{
							"id":"wValidBitsPerSample",
							"label": "Number of valid bits",
							"type": "uint16",
							"endian": "little",
							"requirement": "formatType is not 1 and cbSize >= 2"
						},
						{
							"id":"dwChannelMask",
							"label": "Speaker position mask",
							"type": "uint32",
							"endian": "little",
							"requirement": "formatType is not 1 and cbSize >= 6"
						},
						{
							"id":"SubFormat",
							"label": "GUID, including the data format code",
							"groupSize": "16",
							"requirement": "formatType is not 1 and cbSize == 22",
							"group": [
								{
									"id": "dataFormatCode",
									"label": "Data format code",
									"type": "uint16",
									"endian": "little"
								},
								{
									"id": "subFormatData",
									"label": "SubFormat fixed string",
									"type": "hexa",
									"values": "000000001000800000aa00389b71"
								}
							]
						}
					]
				},
				{
					"id": "factSubChunks",
					"label": "Fact sub-chunk",
					"type": "ascii",
					"values": "fact",
					"endian": "big",
					"groupSize": "factSize + 4 + factSize%2",
					"optional": true,
					"group": [
						{
							"id": "factSize",
							"label": "SubChunk size",
							"type": "uint32",
							"endian": "little"
						},
						{
							"id": "dataSamples",
